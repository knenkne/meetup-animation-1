                <span class="skeleton-profile__icon">
                    <a main="1" class="profile-link-avatar" title="{{message "template.product.profile"}}" href="{{design30.navigation.[profile]}}">
                    <span class="avatar-img">
                        {{#ifNotEq design30.config.[isSbolPro] true}}
                            {{#if design30.data.[profile/info.do].response.detail.avatarPath}}
