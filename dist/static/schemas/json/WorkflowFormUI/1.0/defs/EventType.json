{
  "id": "WorkflowFormUI/1.0/defs/EventType.json",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "EventType": {
      "description": "Exposes available event to form step",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "string",
          "maxLength": 256
        },
        "type": {
          "type": "string",
          "maxLength": 256
        },
        "hidden": {
          "type": "boolean"
        },
        "title": {
          "type": "string",
          "maxLength": 64
        },
        "description": {
          "type": "string",
          "maxLength": 512
        }
      },
      "required": [
        "name"
      ]
    }
  }
}
