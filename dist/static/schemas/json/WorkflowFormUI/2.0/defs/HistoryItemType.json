{
  "id": "WorkflowFormUI/2.0/defs/HistoryItemType.json",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "HistoryItemType": {
      "description": "History item to display breadcrumbs",
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "string",
          "maxLength": 256
        },
        "flow": {
          "type": "string",
          "maxLength": 256
        },
        "flowId": {
          "type": "number",
					"maximum": 1000,
					"minimum": 0
        },
        "state": {
          "type": "string",
          "maxLength": 256
        },
        "title": {
          "type": "string",
          "maxLength": 256
        },
        "value": {
          "type": "string",
          "maxLength": 1024
        },
        "status": {
          "type": "string",
          "enum": [
            "ACTIVE",
            "HIDDEN",
            "DISABLED"
          ]
        }
      },
      "required": [
        "id",
        "flow",
        "state",
        "title",
        "status"
      ]
    }
  }
}
