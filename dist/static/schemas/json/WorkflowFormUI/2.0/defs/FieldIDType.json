{
  "id": "WorkflowFormUI/2.0/defs/FieldIDType.json",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "FieldIDType": {
      "description": "Field id to describe tree-like objects as plain key-value structures",
      "type": "string",
      "pattern": "^(?=.{1,256}$)([A-Za-z0-9][A-Za-z0-9:_|-]*)$",
      "minLength": 1,
      "maxLength": 256
    }
  }
}
