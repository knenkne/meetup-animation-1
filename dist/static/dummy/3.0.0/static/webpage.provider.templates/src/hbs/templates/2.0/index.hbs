<!DOCTYPE html>
<!--[if lt IE 8]> <html lang="ru-RU" class="lt-ie9 lt-ie8 ie7 no-js"> <![endif]-->
<!--[if IE 8]> <html lang="ru-RU" class="lt-ie9 ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="ru-RU" class="ie9 no-js"> <![endif]-->
<!--[if gt IE 9]><!-->
<html lang="ru-RU" class="no-js">
<!--<![endif]-->
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
    <meta name="format-detection" content="telephone=no"/>
    <title>
        {{message "template.page.title"}}
    </title>

    <link rel="preload" href="{{resourcesUrl}}/lib.vendor/{{staticContentVendorVersionSuffix}}/index.js" as="script">
    <link rel="preload" href="{{resourcesUrl}}/{{pageAlias}}/{{staticContentVersionSuffix}}/index.js" as="script">
    <link rel="preload" href="{{resourcesUrl}}/common/{{assetsCommonVersion}}/img/header-logo.svg" as="image">

    {{#if isNewMenuEnabled}}
        <link rel="preload" href="{{navigationStaticUrl}}/index.js" as="script">
        <link rel="preload" href="{{navigationStaticUrl}}/style.css" as="style">
    {{/if}}

    <link rel="apple-touch-icon" sizes="180x180" href="{{resourcesUrl}}/common/{{assetsCommonVersion}}/img/apple-touch-icon.png">
    <link rel="icon" type="image/png" href="{{resourcesUrl}}/common/{{assetsCommonVersion}}/img/favicon-32x32.png" sizes="32x32">
    <link rel="icon" type="image/png" href="{{resourcesUrl}}/common/{{assetsCommonVersion}}/img/favicon-16x16.png" sizes="16x16">
    <link rel="shortcut icon" href="{{resourcesUrl}}/common/{{assetsCommonVersion}}/img/favicon.ico">

    <meta name="apple-mobile-web-app-title" content="Сбербанк Онлайн">
    <meta name="application-name" content="Сбербанк Онлайн">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="{{resourcesUrl}}/common/{{assetsCommonVersion}}/img/mstile-144x144.png">
    <meta name="msapplication-config" content="{{resourcesUrl}}/common/{{assetsCommonVersion}}/cfg/browserconfig.xml">
    <meta name="theme-color" content="#ffffff">

    <script type="text/javascript">
        {{!-- TODO: use toJSON helper --}}
        window.Message = {{{messages}}};
        window.navigation = {{{navigation}}};
        window.config = {{{config}}};
        {{#if modules}}
        window.modules = {{{modules}}};
        {{/if}}
    </script>

    {{!-- staticContentAnalyticsVersionSuffix must be >= 0.2.0 --}}
    {{#if staticContentAnalyticsVersionSuffix}}
    <script src="{{resourcesUrl}}/lib.analytics/{{staticContentAnalyticsVersionSuffix}}/index.js" type="text/javascript"></script>
    {{else}}
    <script type="text/javascript">
        window._rtgParams = {rtgNoSync: true, rtgSyncFrame: false};
    </script>
    <script async src="//mc.yandex.ru/metrika/watch.js" type="text/javascript"></script>
    <script async src="https://www.google-analytics.com/analytics.js" type="text/javascript"></script>
    <script async src="//cdn.rutarget.ru/static/tag/tag.js" type="text/javascript"></script>
    {{/if}}

    <style>
        {{>"2.0/styles/normalize.css"}}
        {{>"2.0/styles/common.css"}}
        {{>"2.0/header/style.css"}}
        {{>"2.0/navigation/style.css"}}
        {{>"2.0/navigation/oldschool.css"}}
        {{>"2.0/styles/style.css"}}
        {{>"2.0/styles/layout.css"}}
        {{>"2.0/fastpayments/style.css"}}
        {{>"2.0/unsupported/style.css"}}
        {{>"2.0/footer/style.css"}}
        {{>"2.0/loader/style.css"}}
    </style>
</head>
<body>
    {{>"2.0/header/index.hbs"}}
    <div class="content">
        <div class="content__center">
            <main>
                {{>"2.0/unsupported/index.hbs"}}
                <div id="app" class="app">
                    {{~#if ssr~}}
                        {{{ssr}}}
                    {{~/if~}}
                </div>
                {{#if dictionaries}}
                    <script>
                        window.__DICTIONARIES = '{{{dictionaries}}}'
                    </script>
                {{/if}}
                {{#if initialState}}
                    <script>
                        window.__INITIAL_STATE = '{{{initialState}}}'
                    </script>
                {{/if}}
                {{>"2.0/loader/index.hbs"}}
            </main>
        </div>
        <aside class="content__left" id="left">
            {{#if showFastPayments}}
                {{>"2.0/fastpayments/index.hbs"}}
            {{/if}}
        </aside>

        <aside class="content__right" id="right"></aside>
    </div>

    {{>"2.0/footer/index.hbs"}}

    <script
        id="config"
        ping-url="{{pingUrl}}"
        base-url="{{apiUrl}}"
        res-url="{{resourcesUrl}}/{{pageAlias}}/{{staticContentVersionSuffix}}"
        user-id="{{loginIdDig}}"
        erib-node="{{eribNode}}"
        erib-main-url="{{eribMainURL}}"

        {{#each allowedServices~}}
        {{@key}}="{{this~}}"
        {{/each}}
        type="text/javascript"
    >
    </script>

    <script src="{{antifraudStaticRoot}}/hashtable.js" type="text/javascript"></script>
    <script src="{{antifraudStaticRoot}}/rsa.js" type="text/javascript"></script>
    {{#if isSecureBankScriptEnabled}}
        <script src="{{resourcesUrl}}/common/{{assetsCommonVersion}}/js/sb.js" type="text/javascript"></script>
    {{/if}}
    <script src="{{resourcesUrl}}/common/{{assetsCommonVersion}}/js/common.js" type="text/javascript"></script>
    <script src="{{resourcesUrl}}/lib.vendor/{{staticContentVendorVersionSuffix}}/index.js" type="text/javascript"></script>
    <script src="{{resourcesUrl}}/{{pageAlias}}/{{staticContentVersionSuffix}}/index.js" type="text/javascript"></script>

    {{#if showChat}}
        <script async src="{{chatUrl}}" type="text/javascript"></script>
    {{/if}}

    <script> if (typeof window.initialized === 'undefined') { document.getElementById('unsupported').style.display = 'block'; stopLoader(); } </script>
    <script> if (typeof window.initLoader === 'undefined') { stopLoader(); } </script>
</body>
</html>
