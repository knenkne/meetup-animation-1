    "lint:style:fix": "npx stylelint \"src/**/*.css\" --fix",
    "optimize:svg": "svgo -f src/icon -r --config=\".svgo.yml\"",
    "production": "npx cross-env NODE_ENV=production CSS_HASH_MAP=true node node_modules/@sbol/webpack-config/bin/build.js",
    "start": "npx cross-env NODE_ENV=development PER_SECTION=false npx styleguidist server",
    "test": "cross-env NODE_ENV=test PKG_ID=lib.ui TESTING=true jest --ci --coverage",
    "test:ui": "npx brokkr --target --chrome-driver-version 2.36",
    "test:ui:dev": "npx brokkr --target --chrome-driver-version 2.36 --debug",
    "test:update": "npm run test -- -u",
    "whitelist:all": "npm run whitelist:audit && npm run whitelist:license && npm run whitelist:required && npm run whitelist:depVersions",
    "whitelist:audit": "npx whitelist -l npmAudit",
    "whitelist:depVersions": "npx whitelist --root --check-list depVersions --lists-path @sbol/whitelist-config/lists",
    "whitelist:license": "npx whitelist -r -l license",
    "whitelist:required": "npx whitelist -r -l required"
