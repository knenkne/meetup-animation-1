    "clean": "npx rimraf target/**/* target-dev/**/*",
    "demo": "npx cross-env NODE_ENV=development CSS_HASH_MAP=true npx styleguidist build",
    "development": "npx cross-env NODE_ENV=development CSS_HASH_MAP=true VIZUALIZER=true node node_modules/@sbol/webpack-config/bin/build.js",
